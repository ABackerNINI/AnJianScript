        FindPic 1150, 75-DeviationY, 1210, 125-DeviationY, "Attachment:close.bmp", "000000",0,0.8, x, y
        If x > 0 and y > 0 Then 
            Exit Function
        End If
        Delay 1000
        
        count = count + 1
        Call L3("FindClose::While "&count,count,20)
    Wend
End Function

Function ShiftTo(i)
    Call L("ShiftTo " & i)
    
    Dim x,y,count = 0
    KeyPress "Home"
    Delay 1000
    
    Do
        FindPic 154, 103 - DeviationY, 224, 169 - DeviationY, "Attachment:阴阳师.bmp", "000000", 0, 0.8, x, y
        If x > 0 And y > 0 Then 
            Exit Do
        End If
        Delay 1000
        
        count = count + 1
        Call L3("ShiftTo::Do "&count,count,20)
    Loop
    
    Dim sum = 0
    While True
        If i = 2 Then 
            Tap 196, 138-DeviationY //阴阳师
        ElseIf i = 3 Then
            Tap 420, 137-DeviationY //#1 阴阳师
        End If
        Delay 1000
    
        count = 0
        Do
            FindPic 154, 103 - DeviationY, 224, 169 - DeviationY, "Attachment:阴阳师.bmp", "000000", 0, 0.8, x, y
            If x < 0 And y < 0 Then 
                Delay 1000
                Exit Function
            End If
            Delay 1000

            If count > 10 Then 
                Exit Do
            End If

            count = count + 1
            Call L("ShiftTo::While::Do "&count)
        Loop
        
        sum = sum + 1
        Call L3("ShiftTo::While "&sum,sum,3)
    Wend
End Function

Function ExpToEnch()
    Call L("ExpToEnch")
    
    Dim x,y
    Delay 2000
    FindPic 1150, 75-DeviationY, 1210, 125-DeviationY, "Attachment:close.bmp", "000000",0,0.8, x, y
    If x > 0 and y > 0 Then 
        Exit Function
    End If
    
    Dim count = 0
    Do
        FindPic 0, 0, 0, 0, "Attachment:结界突破.bmp","000000",0,0.8, x, y
        If x > 0 and y > 0 Then 
            Tap 419,702 - DeviationY //结界突破
            Call FindClose()
            Tap 1232,373 - DeviationY //阴阳寮
            Delay 2000
            Exit Do
        End If
        Delay 1000
        
        If count > 10 Then 
            Exit Do
        End If
        
        count = count + 1
        Call L("ExpToEnch::Do "&count)
    Loop
End Function

Function ChooseLiao(sum)
    Call L("ChooseLiao " & sum)
    
    Delay 1000
    If sum = 1 Then
        Tap 292,209 - DeviationY //第一个寮
    ElseIf sum = 2 Then
        Tap 292,394 - DeviationY //第二个寮
    ElseIf sum = 3 Then
        Tap 292,581 - DeviationY //第三个寮
    End If
    Delay 1000
End Function

Function FindWhichToFight()//查找等级[1-29]
    Call L("FindWhichToFight")
    
    Dim count = 0
    Dim x1 = 813,x2 = 1152,y = 150 - DeviationY
    
    Do
        FindPic x1 - 50,y - 10,x1 + 10,y + 50,"Attachment:未突破.bmp","000000",0,0.9,fight_x,fight_y
        If fight_x > 0 And fight_y > 0 Then 
            FindPic x1 - 157,y + 67,x1 - 120,y + 103,"Attachment:勋章.bmp","000000",0,0.9,fight_x,fight_y
            If fight_x < 0 and fight_y < 0 Then 
                fight_x = x1 - 50
                fight_y = y + 50
                Goto EndFindWhichToFight
            End If
        End If
        
        FindPic x2 - 50,y - 10,x2 + 10,y + 50,"Attachment:未突破.bmp","000000",0,0.9,fight_x,fight_y
        If fight_x > 0 And fight_y > 0 Then 
            FindPic x1 - 157,y + 67,x1 - 120,y + 103,"Attachment:勋章.bmp","000000",0,0.9,fight_x,fight_y
            If fight_x < 0 and fight_y < 0 Then 
                fight_x = x2 - 50
                fight_y = y + 50
                Goto EndFindWhichToFight
            End If
        End If
        
        y = y + 135
        
        If count = 3 Then 
            Exit Do
        End If
        
        count = count + 1
        Call L("FindWhichToFight "&count)
    Loop
    
    FindWhichToFight = 0
    Exit Function
    
    Rem EndFindWhichToFight
    Tap fight_x, fight_y
    Call L("Find "&fight_x&","&fight_y)
    Delay 2500
    
    FindWhichToFight = 1
End Function

Function Restart()
    Call L("Restart")
    KillApp "com.netease.onmyoji"
    KillApp "com.netease.onmyoji.p71bHrVo_1"

    Call Relogin(2)
	
    Call Relogin(3)
    
    //    Call ShiftTo(2)
    
    flag = 1
	
    Call main()
End Function

Function ReLogin(i)
    Call L("Relogin "&i)
    
    Call ShiftTo(i)
    
    Dim x,y,count = 0
    Do
        FindPic 15, 50-DeviationY, 61, 81-DeviationY, "Attachment:活动关闭.bmp", "000000",0,0.8, x, y
        If x > 0 and y > 0 Then 
            Tap 35,66-DeviationY//活动关闭
            Delay 3000
            Tap 1163, 442 - DeviationY//右移
            Delay 1000
        End If
            
        FindPic 1214, 54-DeviationY, 1257, 88-DeviationY, "Attachment:msg.bmp", "000000",0,0.8, x, y
        If x > 0 and y > 0 Then 
            Tap 678,169-DeviationY//探索
            Delay 2000
            Exit Do
        End If
        Tap 639,651-DeviationY//进入游戏
        Delay 1000
        
        count = count + 1
        Call L3("Relogin::Do "&count,count,50)
    Loop
End Function

Function L(s)
    Call L1(s)
End Function

Function L1(s)
    ShowMessage s
    TracePrint s
End Function

Function L2(s,i)
    Call L1(s)
    If (i > 100) Then 
        Call Restart()
    End If
End Function

Function L3(s,i,j)
    Call L1(s)
    If (i > j) Then 
        Call Restart()
    End If
End Function

Function OnScriptExit
    Call L("OnScriptExit")
    Log.Close
End Function

