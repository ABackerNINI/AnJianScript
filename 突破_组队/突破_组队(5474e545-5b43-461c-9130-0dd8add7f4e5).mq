//init

//global vars
Dim DeviationY = 35
Dim fight_x,fight_y
Dim flag = 1
Log.Open 

Call main()
//Call Restart()

Function main()
    Call L("Main")
    
    Call ShiftTo(2)
    
    Dim sum = 1
    
    Dim ret,count = 0
    While true
        Call ExpToEnch()
        
        Call FindClose()
        Call ChooseLiao(sum)
        ret = FindWhichToFight()
        If ret = 1 Then 
            ret = Double()
            If ret = 0 Then 
                Call Single()
            Else 
                Call FindClose()
                Call ChooseLiao(sum)
                ret = FindWhichToFight()
                If ret = 1 Then
                    Call Single()
                End If
            End If
        End If
            
        sum = sum + 1
        If sum = 4 Then 
            Call SecAccount()
            sum = 1
            flag = 0
        End If
        Delay 1000
        
        count = count + 1
        Call L3("Main::While "& count,count,301)//mark
    Wend
End Function

Function SecAccount()
    Call L("SecAccount")
    
    Dim x,y
    Call ShiftTo(3)
    
    FindPic 1150, 75-DeviationY, 1210, 125-DeviationY, "Attachment:close.bmp", "000000",0,0.8, x, y
    If x > 0 and y > 0 Then 
        Tap 1186, 104 - DeviationY //close
        Delay 1000
    End If
    Call ExpToEnch()
    
    Dim sum = 1
    Dim ret,count
    While sum < 4
        count = 0
        Do
            FindPic 1150, 75-DeviationY, 1210, 125-DeviationY, "Attachment:close.bmp", "000000",0,0.8, x, y
            If x > 0 and y > 0 Then 
                Exit Do
            End If
            
            If count > 20 Then 
                Goto EndSecAccount
            End If
            Delay 1000
            
            count = count + 1
            Call L3("SecAccount::While::Do " & count,count,50)
        Loop
        Call ChooseLiao(sum)
        ret = FindWhichToFight()
        If ret = 1 Then
            Call Single()
        End If
        
        Delay 1000
        
        sum = sum + 1
        Call L3("SecAccount::While " & sum,sum,5)
    Wend
    
    Rem EndSecAccount
    Call ShiftTo(2)
    
    Tap 1186, 104 - DeviationY //close
    Delay 2000
    
    Call ExpToEnch()
    
End Function

Function Double()//组队攻击
    Call L("double")
    
    Dim x,y,count = 0
    Do
        FindPic fight_x - 400, fight_y - 300, fight_x + 400, fight_y + 300, "Attachment:组队.bmp", "000000",0, 0.8, x, y
        If x > 0 And y > 0 Then 
            Delay 1000
            Tap x,y
            Delay 2000
            flag = 0
            Exit Do
        Else 
            If flag = 1 Then 
                //Tap 1232,373-DeviationY //阴阳寮
                //Delay 1000
                Double = 0
                Exit Function
            Else
                Delay 2000
            End If
        End If
        
        count = count + 1
        Call L3("double::Do " & count,count,120)
    Loop
    
    FindPic 1150, 75-DeviationY, 1210, 125-DeviationY, "Attachment:close.bmp", "000000",0,0.8, x, y
    If x > 0 and y > 0 Then 
        Exit Function
    End If
    
    Call Invite()

    Call ShiftTo(3)

    Call Accept()
 
    Call ShiftTo(2)

    Call StartFight()
   
    Call DoubleFight()
    
    Double = 1
End Function

Function DoubleFight()
    Call L("DoubleFight")
    
    Call Ready()
    
    Call ShiftTo(3)
    
    Call Ready()
    
    Call Fight()
    
    Call ShiftTo(2)
    
    Call Fight()
End Function

Function Single()//单人攻击
    Call L("single")
    
    Dim x,y,count = 0
    Do
        FindPic fight_x - 400, fight_y - 300, fight_x + 400, fight_y + 300, "Attachment:攻击.bmp", "000000",0,0.8, x, y
        If x > 0 And y > 0 Then 
            Tap x,y
            Delay 2000
            flag = 0
            Exit Do
        Else 
            If flag = 1 Then 
                Tap 1232,373-DeviationY //阴阳寮
                Delay 1000
                Exit Function
            Else
                Delay 2000
            End If
        End If
        
        count = count + 1
        Call L3("single::Do "& count,count,120)
    Loop
    
    Call Ready()
    
    Call Fight()
End Function

